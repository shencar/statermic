\end_layout

\begin_layout Standard
נשתמש באנרגיה הפנימית.
 דיפרנציאל הקשר היסודי:
\begin_inset Formula 
\[
dE=TdS-PdV+\mu dN
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\left(\frac{\partial E}{\partial S}\right)_{V,N} & =T\\
\left(\frac{\partial}{\partial V}\left(\frac{\partial E}{\partial S}\right)_{V,N}\right)_{S,N} & =\left(\frac{\partial T}{\partial V}\right)_{S,N}\\
\left(\frac{\partial}{\partial S}\left(\frac{\partial E}{\partial V}\right)_{S,N}\right)_{V,N} & =\left(\frac{\partial T}{\partial V}\right)_{S,N}\\
-\left(\frac{\partial P}{\partial S}\right)_{V,N} & =\left(\frac{\partial T}{\partial V}\right)_{S,N}\\
-\left(\frac{\partial S}{\partial P}\right)_{V,N} & =\left(\frac{\partial V}{\partial T}\right)_{S,N}
\end{align*}

\end_inset


\end_layout

