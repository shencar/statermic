\end_inset


\end_layout

\begin_layout Standard
נשתמש בפוטנציאל הגרנד קנוני
\begin_inset Formula 
\begin{align*}
\Omega\left(T,V,\mu\right) & =E-TS-\mu N\\
d\Omega & =dE-SdT-TdS-Nd\mu-\mu dN\\
 & =TdS-PdV+\mu dN-SdT-TdS-Nd\mu-\mu dN\\
 & =-PdV-SdT-Nd\mu\\
\left(\frac{\partial\Omega}{\partial V}\right)_{T,\mu} & =-P\\
\left(\frac{\partial}{\partial\mu}\left(\frac{\partial\Omega}{\partial V}\right)_{T,\mu}\right)_{T,V} & =-\left(\frac{\partial P}{\partial\mu}\right)_{T,V}\\
\left(\frac{\partial}{\partial V}\left(\frac{\partial\Omega}{\partial\mu}\right)_{T,V}\right)_{T,\mu} & =-\left(\frac{\partial P}{\partial\mu}\right)_{T,V}\\
-\left(\frac{\partial N}{\partial V}\right)_{T,\mu} & =-\left(\frac{\partial P}{\partial\mu}\right)_{T,V}\\
\left(\frac{\partial N}{\partial V}\right)_{T,\mu} & =\left(\frac{\partial P}{\partial\mu}\right)_{T,V}
\end{align*}

\end_inset


\end_layout

\begin_layout Subsubsection
